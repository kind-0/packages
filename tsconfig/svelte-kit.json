{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "SvelteKit",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "ES2017",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}